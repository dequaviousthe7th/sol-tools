{
  "name": "solreclaimer",
  "version": "1.2.0",
  "private": true,
  "description": "SolReclaimer - Zero-fee Solana rent reclaimer - close empty token accounts and reclaim SOL",
  "author": "Dequavious",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "pnpm --filter web dev",
    "dev:cli": "pnpm --filter cli dev",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "dev:worker": "pnpm --filter @solreclaimer/worker dev",
    "deploy:worker": "pnpm --filter @solreclaimer/worker deploy",
    "deploy": "pnpm build && npx wrangler pages deploy packages/web/out --project-name=solreclaimer"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0"
  }
}
